{
	"manifest_version": 2,
	"name": "Impulse",
	"version": "0.0.1",
	"browser_action":{
		"default_popup": "popup.html",
		"default_title": "Impulse"
	},
	"background": {
        "scripts": ["background.js"]
	},
	"options_page": "Impulse.html",
    "permissions":[
        "tabs",
		"contextMenus",
		"storage"
    ]
}